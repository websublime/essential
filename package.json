    "@microsoft/api-documenter": "^7.11.0",
    "@microsoft/api-extractor": "^7.12.0",
    "@types/fs-extra": "^9.0.4",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.11.12",
    "esbuild": "^0.14.23",
    "eslint": "^8.13.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "fs-extra": "^10.0.1",
