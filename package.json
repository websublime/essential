{
  "name": "@websublime/essential",
  "version": "0.0.3",
  "source": "./src",
  "main": "./dist/essential.cjs.js",
  "module": "./dist/essential.esm.js",
  "types": "./dist/@types/index.d.ts",
  "files": [
    "dist/",
    "./LICENSE.md",
    "./README.md"
  ],
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
